	<queryString>
		<![CDATA[SELECT
   $P!{PARAM_tabla}.codpago as codformapago,
   formaspago.domiciliado as domiciliado,
   cuentasbcocli.entidad as cliente_entidad,
   cuentasbcocli.ctaagencia as cliente_oficina,
   cuentasbcocli.cuenta as cliente_cuenta,
   (SELECT entidad FROM cuentasbanco WHERE codcuenta = (SELECT codcuenta FROM formaspago WHERE codpago = $P!{PARAM_tabla}.codpago)) as empresabco_entidad,
   (SELECT ctaagencia FROM cuentasbanco WHERE codcuenta = (SELECT codcuenta FROM formaspago WHERE codpago = $P!{PARAM_tabla}.codpago)) as empresabco_oficina,
   (SELECT cuenta FROM cuentasbanco WHERE codcuenta = (SELECT codcuenta FROM formaspago WHERE codpago = $P!{PARAM_tabla}.codpago)) as empresabco_cuenta
FROM $P!{PARAM_tabla}
LEFT OUTER JOIN clientes ON facturascli.codcliente = clientes.codcliente
LEFT OUTER JOIN formaspago ON $P!{PARAM_tabla}.codpago = formaspago.codpago
LEFT OUTER JOIN cuentasbcocli ON $P!{PARAM_tabla}.codcliente = cuentasbcocli.codcliente
WHERE cuentasbcocli.codcuenta = clientes.codcuentadom AND $P!{WHERE}]]>
	</queryString>
