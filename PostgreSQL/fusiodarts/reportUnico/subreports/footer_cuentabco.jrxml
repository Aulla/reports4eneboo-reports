	</variable>
	<variable name="empresa_cuentabco" class="java.lang.String">
		<variableExpression><![CDATA[$F{empresabco_entidad} + "  " + $F{empresabco_oficina} + "  " + $V{calculaDC} + "  " + $F{empresabco_cuenta}]]></variableExpression>
	</variable>
