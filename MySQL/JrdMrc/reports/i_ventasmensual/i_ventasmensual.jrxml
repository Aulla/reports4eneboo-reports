	<parameter name="WHERE" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["fecha >= '2012-01-01T00:00:00' AND fecha <= '2012-12-31T00:00:00'  AND codejercicio = '2012'"]]></defaultValueExpression>
	</parameter>
